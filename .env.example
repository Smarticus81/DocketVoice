# DocketVoice Configuration
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key - Required for voice and AI features
# Get your key from: https://platform.openai.com/account/api-keys
OPENAI_API_KEY=sk-your-openai-api-key-here

# Optional: Other AI providers
ANTHROPIC_API_KEY=your-anthropic-key-here

# Voice Configuration
VOICE_PRIMARY_PROVIDER=openai
VOICE_ELEVENLABS_API_KEY=your-elevenlabs-key-here
VOICE_AZURE_SPEECH_KEY=your-azure-speech-key-here

# Environment
ENVIRONMENT=development
DEBUG=true

# Agent Configuration
AGENT_ENABLE_VOICE=true
AGENT_MAX_CONVERSATION_LENGTH=100
